<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
    <WarningsAsErrors>nullable</WarningsAsErrors>
    <PackOnBuild>true</PackOnBuild>
    <Authors>Roman S</Authors>
    <Description>Heir to Crassula, but with new ideas and full support for NET 5.</Description>
    <Copyright>Roman S</Copyright>
    <Owners>Roman S</Owners>
    <Title>Rovecode.Lotos</Title>
    <NeutralLanguage>en-US</NeutralLanguage>
    <ReleaseVersion>0.2.0</ReleaseVersion>
    <PackageVersion>0.1.0</PackageVersion>
    <SynchReleaseVersion>false</SynchReleaseVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MongoDB.Driver" Version="2.11.5" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Repositories\" />
    <Folder Include="Models\" />
    <Folder Include="Enums\" />
    <Folder Include="Contexts\" />
    <Folder Include="Providers\" />
    <Folder Include="Exceptions\" />
    <Folder Include="Factories\" />
    <Folder Include="Extensions\" />
  </ItemGroup>
</Project>
